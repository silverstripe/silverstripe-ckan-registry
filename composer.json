{
    "name": "silverstripe/ckan-registry",
    "description": "SilverStripe CKAN registry integration",
    "type": "silverstripe-vendormodule",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "ckan",
        "cwp"
    ],
    "authors": [
        {
            "name": "SilverStripe",
            "homepage": "https://www.silverstripe.com"
        },
        {
            "name": "The SilverStripe Community",
            "homepage": "https://www.silverstripe.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "silverstripe/cms": "^5",
        "silverstripe/admin": "^2",
        "silverstripe/vendor-plugin": "^2",
        "symbiote/silverstripe-gridfieldextensions": "^4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\CKANRegistry\\": "src/",
            "SilverStripe\\CKANRegistry\\Tests\\": "tests/",
            "SilverStripe\\CKANRegistry\\Tests\\Behat\\Context\\": "tests/behat/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}